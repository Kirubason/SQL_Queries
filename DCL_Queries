
GRANT
------------------

CREATE USER TESTING_SCHEMA IDENTIFIED BY TEST 

GRANT CREATE SESSION TO TESTING_SCHEMA

GRANT CREATE TABLE TO TESTING_SCHEMA

SELECT DEFAULT_TABLESPACE FROM DBA_USERS WHERE  USERNAME = 'TESTING_SCHEMA';

ALTER USER TESTING_SCHEMA QUOTA UNLIMITED ON SYSTEM;

GRANT SELECT, INSERT, UPDATE, DELETE ON TESTING_SCHEMA.TESTTABLE
TO TESTING_SCHEMA;



REVOKE
----------------------------


REVOKE CREATE TABLE FROM TESTING_SCHEMA;

REVOKE CREATE SESSION FROM TESTING_SCHEMA;
